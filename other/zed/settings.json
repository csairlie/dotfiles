{
  "project_panel": {
    "button": false,
    "dock": "right",
    "git_status": false
  },
  "outline_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "chat_panel": {
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "assistant": {
    "button": false,
    "enabled": false
  },
  "git": {
    "git_gutter": "hide",
    "inline_blame": {
      "enabled": false
    }
  },
  "theme": "One Dark",
  "features": {
    "copilot": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "scrollbar": {
    "show": "never"
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
    "selections_menu": false
  },
  "gutter": {
    "code_actions": false
  },
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "preview_tabs": {
    "enabled": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "vim_mode": true,
  "cursor_blink": false,
  "relative_line_numbers": true,
  "enable_language_server": true,
  "current_line_highlight": "none",
  "autosave": "on_window_change",
  "extend_comment_on_newline": false,
  "auto_install_extensions": {
    "java": true,
    "zed-java-language-support-jdtls": true
  }
}
